<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.reyoung.dao.fdao.FnameDao">

    <select id="findallfname" resultMap="fnameMap">

        SELECT * FROM fname;

    </select>

    <resultMap id="fnameMap" type="com.reyoung.model.filter.Fname">

        <id column="filterid" property="filterid" />
        <result column="filtername" property="filtername" />


    </resultMap>

</mapper>