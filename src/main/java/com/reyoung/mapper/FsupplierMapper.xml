<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.reyoung.dao.fdao.FsupplierDao">

    <select id="findallfsupplier" resultMap="fsupplierMap">

        SELECT * FROM fsupplier;

    </select>


    <resultMap id="fsupplierMap" type="com.reyoung.model.filter.Fsupplier">

        <id column="fsupplierid" property="fsupplierid" />
        <result column="fsuppliername" property="fsuppliername" />

    </resultMap>

</mapper>